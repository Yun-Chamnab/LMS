;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Header.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"\">\n    <div\n      class=\"sidebar\"\n      v-on:mouseover=\"onMouseOver\"\n      v-on:mouseleave=\"onMouseLeave\"\n    >\n      <v-navigation-drawer\n        style=\"background-color: #0306b9\"\n        v-model=\"drawer\"\n        :mini-variant=\"miniVariant\"\n        :expand-on-hover=\"handOver\"\n        :mini-variant-width=\"60\"\n        :app=\"isApp\"\n        :fixed=\"isAbsolute\"\n        color=\"\"\n      >\n        <v-list dense class=\"pa-0\">\n          <span class=\"hidden-sm-and-down\">\n            <a v-if=\"isAccountLogo\" href=\"/\" class=\"main_logo pl-7 pt-1\">\n              <h1 class=\"mb-0\">\n                <img\n                  src=\"../assets/images/NIPTICT-Webiste-logo-1.png\"\n                  height=\"75\"\n                  alt=\"Niptict Logo\"\n                />\n              </h1>\n            </a>\n            <a v-if=\"!isAccountLogo\" href=\"/\" class=\"\">\n              <h1 class=\"mb-0\">\n                <img\n                  class=\"small_img fade-in\"\n                  src=\"../assets/images/NIPTICT-Webiste-logo-1.png\"\n                  height=\"25\"\n                  alt=\"Niptict Logo\"\n                />\n              </h1>\n            </a>\n            <span\n              v-bind:class=\"{\n                angle_action_small: miniVariant,\n                angle_action: !miniVariant,\n              }\"\n            >\n              <v-icon\n                size=\"16\"\n                class=\"arr_icon\"\n                @click=\"hideSidebar\"\n                v-if=\"!miniVariant\"\n                >fa fa-chevron-circle-left</v-icon\n              >\n              <v-icon\n  
       dark\r\n            v-bind=\"attrs\"\r\n            v-on=\"on\"\r\n          >\r\n            Create Quiz\r\n          </v-btn>\r\n        </template>\r\n\r\n        <v-card>\r\n          <v-card-title>\r\n            <span class=\"headline\">Create Quiz</span>\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-container>\r\n              <v-row>\r\n                <v-col cols=\"12\">\r\n                  <v-text-field\r\n                    label=\"Title\"\r\n                    v-model=\"title\"\r\n                    required\r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <!-- <v-text-field\r\n                    label=\"Duration\"\r\n                    v-model=\"duration\"\r\n                    required\r\n                  ></v-text-field> -->\r\n                  <h4>Duration</h4>\r\n                  <span class=\"border\">\r\n                    <v-digital-time-picker\r\n                      v-model=\"duration\"\r\n                      rounded\r\n                      filled\r\n                      outlined\r\n                      color=\"info\"\r\n                    />\r\n                  </span>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <v-checkbox\r\n                    v-model=\"publish\"\r\n                    :label=\"`Publish: ${publish.toString()}`\"\r\n                  ></v-checkbox>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"\r\n                dialog = false;\r\n                clear();\r\n              \"\r\n            >\r\n              Close\r\n            </v-btn>\r\n            <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"\r\n                dialog = false;\r\n                onSaveClose();\r\n              \"\r\n            >\r\n              Save\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </v-row>\r\n\r\n    <v-row>\r\n      <div\r\n        class=\"mx-3 mt-3\"\r\n        v-for=\"(item, i) in items\"\r\n        :key=\"i\"\r\n        cols=\"12\"\r\n        sm=\"4\"\r\n      >\r\n        <!-- Delete Dialog -->\r\n        <v-dialog v-model=\"dialog2\" max-width=\"290\">\r\n          <v-card>\r\n            <v-card-title class=\"headline\">Are you sure?</v-card-title>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n\r\n              <v-btn color=\"green darken-1\" text @click=\"dialog2 = false\">\r\n                Cancel\r\n              </v-btn>\r\n\r\n              <v-btn\r\n                color=\"green darken-1\"\r\n                text\r\n                @click=\"deleteExam(item.id)\"\r\n                @click.prevent=\"dialog2 = false\"\r\n              >\r\n                Agree\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <!-- =====END delete====== -->\r\n        <v-card\r\n          class=\"rounded\"\r\n          outlined\r\n          tile\r\n          color=\"light-blue accent-4\"\r\n          dark\r\n          min-width=\"230px\"\r\n        >\r\n          <i\r\n            class=\"far fa-trash-alt float-right mx-1\"\r\n            @click.stop=\"dialog2 = true\"\r\n          ></i>\r\n          <i\r\n            class=\"far fa-edit float-right\"\r\n            color=\"primary\"\r\n            @click=\"onEditItem(item)\"\r\n          ></i>\r\n\r\n          <v-card-title class=\"headline\"> {{ item.name }} </v-card-title>\r\n          <v-card-subtitle text-center\r\n            ><i class=\"fas fa-stopwatch fa-2x\" style=\"color: white\"></i>\r\n            <h3>{{ item.duration }}</h3></v-card-subtitle\r\n          >\r\n          <v-card-subtitle>Publish: {{ item.publish }}</v-card-subtitle>\r\n\r\n          <router-link\r\n            style=\"display: inline-block; text-decoration: none; float: right\"\r\n            :to=\"'question/' + item.id\"\r\n          >\r\n            <i\r\n              class=\"fas fa-plus-circle fa-2x\"\r\n              @click=\"$router.push({ name: 'test2', params: { id: item.id } })\"\r\n            ></i>\r\n          </router-link>\r\n        </v-card>\r\n      </div>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nimport DialogQuestion from \"./DialogQuestion\";\r\nimport TakeExam from \"./TakeExam\";\r\n\r\nconst apiUrl = require(\"../../apiUrl.js\");\r\nimport { VDigitalTimePicker } from \"v-digital-time-picker\";\r\n\r\nexport default {\r\n  components: {\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    DialogQuestion,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    TakeExam,\r\n    VDigitalTimePicker,\r\n  },\r\n  data: () => ({\r\n    items: [],\r\n    dialog: false,\r\n    dialog1: false,\r\n    dialog2: false,\r\n    e1: 1,\r\n    examId: \"\",\r\n    title: null,\r\n    duration: \"\",\r\n    timeValue: \"\",\r\n    publish: false,\r\n  }),\r\n  async mounted() {\r\n    await this.loadData();\r\n  },\r\n  methods: {\r\n    async loadData() {\r\n      axios\r\n        .get(\"http://127.0.0.1:8000/api/exam\")\r\n        .then((response) => {\r\n          this.items = response.data.data;\r\n          window.console.log(this.items);\r\n        })\r\n        .catch((error) => {\r\n          window.console.log(error);\r\n        });\r\n    },\r\n    async onEditItem(exam) {\r\n      this.title = exam.name;\r\n      this.duration = exam.duration;\r\n      this.publish = exam.publish;\r\n      this.examId = exam.id;\r\n      this.dialog = true;\r\n    },\r\n    async onSaveClose() {\r\n      new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          resolve(\"resolved\");\r\n\r\n          let strUrl = apiUrl.exam_post;\r\n          let method = \"post\";\r\n\r\n          if (this.examId !== \"\") {\r\n            strUrl = apiUrl.exam_edit + \"/\" + this.examId;\r\n            method = \"patch\";\r\n          }\r\n          axios({\r\n            method: method,\r\n            url: strUrl,\r\n            data: {\r\n              name: this.title,\r\n              duration: this.duration,\r\n              publish: this.publish,\r\n            },\r\n          })\r\n            .then((response) => {\r\n              this.loadData();\r\n              this.clear();\r\n              window.console.log(response);\r\n            })\r\n            .catch((e) => {\r\n              this.errors.push(e);\r\n            });\r\n        }, 200);\r\n      });\r\n    },\r\n    openModal() {\r\n      this.$refs.modal.showModal();\r\n    },\r\n    clear() {\r\n      (this.title = \"\"), (this.duration = \"\"), (this.publish = false);\r\n    },\r\n    async deleteExam(id) {\r\n      axios\r\n        .delete(\"http://127.0.0.1:8000/api/exam/\" + id)\r\n        .then(function (response) {\r\n          window.console.log(response);\r\n        })\r\n        .catch(function (error) {\r\n          window.console.log(error);\r\n        });\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>"]}]}
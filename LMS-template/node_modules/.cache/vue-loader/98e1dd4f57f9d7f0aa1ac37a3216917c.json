d-text>\r\n            <v-container>\r\n              <v-row>\r\n                <v-col cols=\"12\">\r\n                  <v-text-field\r\n                    label=\"Title\"\r\n                    v-model=\"title\"\r\n                    required\r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <v-text-field\r\n                    label=\"Duration\"\r\n                    v-model=\"duration\"\r\n                    required\r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <v-text-field\r\n                    label=\"Publish\"\r\n                    v-model=\"publish\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n            <!-- <small>*indicates required field</small> -->\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\r\n              Close\r\n            </v-btn>\r\n            <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click.prevent=\"onSaveClose()\"\r\n              @click=\"dialog = false\"\r\n            >\r\n              Save\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </v-row>\r\n\r\n    <v-row>\r\n      <div v-for=\"(item, i) in items\" :key=\"i\" cols=\"12\" sm=\"4\">\r\n        <v-card\r\n          class=\"mx-3 mb-3 mt-3 pa-2 rounded\"\r\n          outlined\r\n          tile\r\n          color=\"#385F73\"\r\n          dark\r\n        >\r\n          <v-card-title class=\"headline\"> {{ item.name }} </v-card-title>\r\n          <v-card-subtitle>Duration: {{ item.duration }}</v-card-subtitle>\r\n          <v-card-subtitle>Publish: {{ item.publish }}</v-card-subtitle>\r\n          <v-card-actions>\r\n            <v-dialog v-model=\"dialog1\">\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-btn v-on=\"on\"> Create Question </v-btn>\r\n              </template>\r\n            </v-dialog>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </div>\r\n    </v-row>\r\n    <dialog-container\r\n      v-if=\"productInEdit\"\r\n      :data-item=\"productInEdit\"\r\n      @save=\"save\"\r\n      @cancel=\"cancel\"\r\n    >\r\n    </dialog-container>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { Grid, GridToolbar, GridNoRecords } from \"@progress/kendo-vue-grid\";\r\nimport DialogQuestion from \"./DialogQuestion\";\r\n\r\nconst apiUrl = require(\"../../apiUrl.js\");\r\n\r\nexport default {\r\n  components: {\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    'Grid': Grid,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    \"grid-toolbar\": GridToolbar,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    \"grid-norecords\": GridNoRecords,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    \"dialog-question\": DialogQuestion,\r\n  },\r\n  data: () => ({\r\n    items: [],\r\n    dialog: false,\r\n    dialog1: false,\r\n  }),\r\n  async mounted() {\r\n    await this.loadData();\r\n  },\r\n  methods: {\r\n    async loadData() {\r\n      axios\r\n        .get(\"http://127.0.0.1:8000/api/exam\")\r\n        .then((response) => {\r\n          this.items = response.data.data;\r\n          window.console.log(this.items);\r\n          // this.loading = false;\r\n        })\r\n        .catch((error) => {\r\n          window.console.log(error);\r\n        });\r\n    },\r\n    async onSaveClose() {\r\n      new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          resolve(\"resolved\");\r\n\r\n          let strUrl = apiUrl.exam_post;\r\n          let method = \"post\";\r\n          axios({\r\n            method: method,\r\n            url: strUrl,\r\n            data: {\r\n              name: this.title,\r\n              duration: this.duration,\r\n              publish: this.publish,\r\n              //   question: this.question,\r\n            },\r\n          })\r\n            .then((response) => {\r\n              this.loadData();\r\n              window.console.log(response);\r\n            })\r\n            .catch((e) => {\r\n              this.errors.push(e);\r\n            });\r\n        }, 200);\r\n      });\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>"]}]}
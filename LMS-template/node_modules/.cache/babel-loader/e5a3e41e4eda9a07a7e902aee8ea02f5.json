AAA;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,iBATA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAZA;AAaA,IAAA,WAbA,yBAaA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,kBAAA,UAAA,CAAA,YAAA;AACA,oBAAA,OAAA,CAAA,UAAA,CAAA;AAEA,wBAAA,MAAA,GAAA,MAAA,CAAA,SAAA;AACA,wBAAA,MAAA,GAAA,MAAA;AACA,oBAAA,KAAA,CAAA;AACA,sBAAA,MAAA,EAAA,MADA;AAEA,sBAAA,GAAA,EAAA,MAFA;AAGA,sBAAA,IAAA,EAAA;AACA,wBAAA,IAAA,EAAA,MAAA,CAAA,KADA;AAEA,wBAAA,QAAA,EAAA,MAAA,CAAA,QAFA;AAGA,wBAAA,OAAA,EAAA,MAAA,CAAA,OAHA,CAIA;;AAJA;AAHA,qBAAA,CAAA,CAUA,IAVA,CAUA,UAAA,QAAA,EAAA;AACA,sBAAA,MAAA,CAAA,QAAA;;AACA,sBAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,qBAbA,EAcA,KAdA,CAcA,UAAA,CAAA,EAAA;AACA,sBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,qBAhBA;AAiBA,mBAtBA,EAsBA,GAtBA,CAAA;AAuBA,iBAxBA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BA,KAvCA;AAwCA,IAAA,SAxCA,uBAwCA;AACA,WAAA,KAAA,CAAA,KAAA,CAAA,SAAA;AACA;AA1CA,GAbA;AAyDA,EAAA,QAAA,EAAA;AAzDA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"grey lighten-5\">\r\n    <v-row justify=\"center\">\r\n      <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n          <v-btn color=\"success\" dark v-bind=\"attrs\" v-on=\"on\">\r\n            Create Quiz\r\n          </v-btn>\r\n        </template>\r\n        <v-card>\r\n          <v-card-title>\r\n            <span class=\"headline\">Create Quiz</span>\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-container>\r\n              <v-row>\r\n                <v-col cols=\"12\">\r\n                  <v-text-field\r\n                    label=\"Title\"\r\n                    v-model=\"title\"\r\n                    required\r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <v-text-field\r\n                    label=\"Duration\"\r\n                    v-model=\"duration\"\r\n                    required\r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                  <v-text-field\r\n                    label=\"Publish\"\r\n                    v-model=\"publish\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n            </v-container>\r\n            <!-- <small>*indicates required field</small> -->\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">\r\n              Close\r\n            </v-btn>\r\n            <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click.prevent=\"onSaveClose()\"\r\n              @click=\"dialog = false\"\r\n            >\r\n              Save\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </v-row>\r\n\r\n    <v-row>\r\n      <div v-for=\"(item, i) in items\" :key=\"i\" cols=\"12\" sm=\"4\">\r\n        <v-card\r\n          class=\"mx-3 mb-3 mt-3 pa-2 rounded\"\r\n          outlined\r\n          tile\r\n          color=\"#385F73\"\r\n          dark\r\n        >\r\n          <v-card-title class=\"headline\"> {{ item.name }} </v-card-title>\r\n          <v-card-subtitle>Duration: {{ item.duration }}</v-card-subtitle>\r\n          <v-card-subtitle>Publish: {{ item.publish }}</v-card-subtitle>\r\n          <v-card-actions>\r\n            <v-dialog>\r\n              <template>\r\n                <v-btn @click=\"openModal\"> Create Question </v-btn>\r\n                <tenant-number-modal ref=\"modal\"></tenant-number-modal>\r\n              </template>\r\n            </v-dialog>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </div>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n// import { Grid, GridToolbar, GridNoRecords } from \"@progress/kendo-vue-grid\";\r\nimport DialogQuestion from \"./DialogQuestion\";\r\n\r\nconst apiUrl = require(\"../../apiUrl.js\");\r\n\r\nexport default {\r\n  components: {\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    DialogQuestion,\r\n  },\r\n  data: () => ({\r\n    items: [],\r\n    dialog: false,\r\n    dialog1: false,\r\n  }),\r\n  async mounted() {\r\n    await this.loadData();\r\n  },\r\n  methods: {\r\n    async loadData() {\r\n      axios\r\n        .get(\"http://127.0.0.1:8000/api/exam\")\r\n        .then((response) => {\r\n          this.items = response.data.data;\r\n          window.console.log(this.items);\r\n          // this.loading = false;\r\n        })\r\n        .catch((error) => {\r\n          window.console.log(error);\r\n        });\r\n    },\r\n    async onSaveClose() {\r\n      new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          resolve(\"resolved\");\r\n\r\n          let strUrl = apiUrl.exam_post;\r\n          let method = \"post\";\r\n          axios({\r\n            method: method,\r\n            url: strUrl,\r\n            data: {\r\n              name: this.title,\r\n              duration: this.duration,\r\n              publish: this.publish,\r\n              //   question: this.question,\r\n            },\r\n          })\r\n            .then((response) => {\r\n              this.loadData();\r\n              window.console.log(response);\r\n            })\r\n            .catch((e) => {\r\n              this.errors.push(e);\r\n            });\r\n        }, 200);\r\n      });\r\n    },\r\n    openModal() {\r\n      this.$refs.modal.showModal();\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>"],"sourceRoot":"src/views/exam"}]}